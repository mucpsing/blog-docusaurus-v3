{
  "name": "cps-blog-docusaurus-v3",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 3001",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "changelog1": "npx conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "changelog2": "npx conventional-changelog -p -i CHANGELOG.md -s -r 0",
    "git": "npx conventional-changelog -p -i CHANGELOG.md -s -r 0 & git add . & git cz",
    "push": "npx conventional-changelog -p -i CHANGELOG.md -s -r 0 & git add . & git cz & git push origin master"
  },
  "dependencies": {
    "@docusaurus/core": "3.1.1",
    "@docusaurus/preset-classic": "3.1.1",
    "@docusaurus/theme-mermaid": "^3.1.1",
    "@mdx-js/react": "^3.0.0",
    "@types/conventional-changelog": "^3.1.5",
    "clsx": "^2.0.0",
    "cz-custom": "^0.0.2",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.01.1",
    "@docusaurus/tsconfig": "3.1.1",
    "@docusaurus/types": "3.1.1",
    "autoprefixer": "^10.4.17",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "postcss": "^8.4.35",
    "typescript": "~5.2.2",
    "yaml": "^2.3.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-custom"
    }
  }
}
